{
    "expo": {
        "name": "Pingua",
        "slug": "pingua",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./src/assets/images/icon.png",
        "scheme": "pingua",
        "userInterfaceStyle": "light",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.latinary.pingua",
            "buildNumber": "1"
        },
        "android": {
            "package": "com.latinary.pingua",
            "versionCode": 1,
            "adaptiveIcon": {
                "foregroundImage": "./src/assets/images/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            }
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./src/assets/images/favicon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./src/assets/images/splash-icon.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#7d51ef"
                }
            ],
            "expo-video",
            "expo-localization"
        ],
        "experiments": {
            "typedRoutes": true
        }
    }
}
